combos {
    compatible = "zmk,combos";
    
    combo_space {
        timeout-ms = <50>;
        key-positions = <19 20 21 22>;
        bindings = <&kp SPACE>;
    };
    
    combo_enter {
        timeout-ms = <50>;
        key-positions = <13 14 15 16>;
        bindings = <&kp ENTER>;
    };
    
    combo_tab {
        timeout-ms = <50>;
        key-positions = <2 3 4>;
        bindings = <&kp TAB>;
    };
    
    combo_backspace {
        timeout-ms = <50>;
        key-positions = <19 20 21>;
        bindings = <&kp BACKSPACE>;
    };
    
    combo_delete {
        timeout-ms = <50>;
        key-positions = <7 8 9>;
        bindings = <&kp DELETE>;
    };
};